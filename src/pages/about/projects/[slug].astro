---
import { data } from '@content/projects/projects.json';
import BaseLayout from '@layouts/Base.astro';
import ProjectDetailContent from '@components/pages/Projects/index.astro';

export function getStaticPaths() {
  return data;
}

const { slug } = Astro.params;
const props = Astro.props;
---

<BaseLayout
  title={slug}
  description={`Efficient description the time I worked on ${props.name}`}
>
  <ProjectDetailContent {...props} />
</BaseLayout>
